{
  "name": "@shirterp/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch --preserveWatchOutput",
    "clean": "rm -rf .turbo node_modules",
    "format": "biome format ./src",
    "lint": "biome lint ./src",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@trpc/server": "11.0.0-rc.477",
    "superjson": "2.2.1",
    "@lucia-auth/adapter-drizzle": "1.1.0",
    "@t3-oss/env-nextjs": "0.11.0",
    "@shirterp/email-templates": "workspace:*",
    "lucia": "3.2.0",
    "next": "14.2.5",
    "oslo": "1.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@shirterp/tsconfig": "workspace:*",
    "typescript": "5.5.4"
  }
}